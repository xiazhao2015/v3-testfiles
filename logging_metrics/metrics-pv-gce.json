{
  "apiVersion": "v1",
  "kind": "PersistentVolume",
  "metadata": {
    "name": "gce-metrics"
  },
  "spec": {
    "capacity": {
        "storage": "10Gi"
    },
    "accessModes": [ "ReadWriteOnce" ],
    "gcePersistentDisk": {
        "pdName": "gcepd-metrics",
        "fsType": "ext4"
    },
    "persistentVolumeReclaimPolicy": "Default"
  }
}

